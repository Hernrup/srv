{
  consul: "127.0.0.1:8500",
  logging: {
    level: "INFO",
    format: "default",
    output: "stdout"
  },
  jobs: [
    {
      name: "preStart",
    },
    {
      name: "consul",
      port: 8500,
      exec: [
        "/bin/consul", "agent", "-server", "-ui",
        "-bootstrap-expect", "1", "-config-dir=/etc/consul"
        ],
      when: {
        source: "preStart",
        once: "exitSuccess"
      },
    },
    {
      name: "preStop",
      exec: ["consul", "leave"],
      when: {
        source: "consul",
        once: "stopping"
      }
    }
  ],
}
