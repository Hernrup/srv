{
  consul: "127.0.0.1:8500",
  logging: {
    level: "INFO",
    format: "default",
    output: "stdout"
  },
  jobs: [
    {
      name: "consul",
      exec: [
        "/bin/consul", "agent", "-server", "-ui", "-dev", "-config-dir=/etc/consul"],
    },
    {
      name: "preStop",
      exec: ["consul", "leave"],
      when: {
        source: "consul",
        once: "stopping"
      }
    }
  ],
}
